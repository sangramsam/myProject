#include<stdio.h>
#include<conio.h>
typedef struct node_type
{
int data;
struct node_type *next;
}node;
typedef node *list;
main()
{
list head,temp,temp1;
char ch;
int n,ptr;
head=NULL;
clrscr();
printf("enter data ?(y/n) \n");
ch=getche();
fflush(stdin);
while(ch=='y'||ch=='Y')
{
printf("give data:");
scanf("%d",&n);
temp=(list)malloc(sizeof(node));
temp->data=n;
temp->next=head;
head=temp;
printf("enter more data ? (y/n) \n");
scanf("\n%c",&ch);
fflush(stdin);
}
temp=head;
temp1=head;
while(temp->next!=NULL)
{
temp1=temp;
temp=temp->next;
if(temp1->data<temp->data)
{
ptr=temp1->data;
temp1->data=temp->data;
temp->data=ptr;
}
}
temp=head;
while(temp!=NULL)
{
printf("%d\t",temp->data);
temp=temp->next;
}
getch();
}
